CREATE TABLE documentEntity (
  id TEXT PRIMARY KEY,
  title TEXT NOT NULL,
  created_at INTEGER NOT NULL,
  last_updated_at INTEGER NOT NULL,
  user_id TEXT NOT NULL
);


selectAll:
SELECT *
FROM documentEntity;

insert:
INSERT INTO documentEntity(id, title, created_at, last_updated_at, user_id)
VALUES (?, ?, ?, ?, ?);